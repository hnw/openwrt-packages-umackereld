{
    "package": {
        "subject": "hnw",
        "repo": "openwrt-packages",
        "name": "${PKG_NAME}",
        "desc": "A small Mackerel agent written in C for OpenWrt",
        "website_url": "https://github.com/hanazuki/umackereld/",
        "issue_tracker_url": "https://github.com/hnw/openwrt-packages-${PKG_NAME}/issues",
        "vcs_url": "https://github.com/hnw/openwrt-packages-${PKG_NAME}.git",
        "licenses": ["Apache-2.0"],
        "public_download_numbers": false,
        "public_stats": false
    },
    "version": {
        "name": "${VERSION}",
        "desc": "${DESC}",
	"vcs_tag": "${VCS_TAG}",
        "released": "${RELEASED}",
        "github_use_tag_release_notes": false,
        "gpgSign": false
    },
    "files": [
	{
	    "includePattern": "${TRAVIS_BUILD_DIR}/pkgs-for-bintray/(.*\\.ipk)",
	    "uploadPattern": "${DISTRO}/${ARCH}${UPLOAD_DIR_SUFFIX}/$1",
	    "matrixParams": { "override": 1 }
	}
    ],
    "publish": true
}
